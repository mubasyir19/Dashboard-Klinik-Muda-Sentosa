<table id="example1" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No.</th>
      <th>Penanya</th>
      <th>Pertanyaan</th>
      <th>Jawaban</th>
      <th>Dokter</th>
      <th>Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% for(let i = 0; i < getConsultationData.length; i++) { %>
    <tr>
      <td><%= i + 1 %></td>
      <td><%= getConsultationData[i].asker %></td>
      <td><%= getConsultationData[i].question %></td>
      <td><%= getConsultationData[i].answer %></td>
      <td><%= getConsultationData[i]?.Account?.name %></td>
      <td>
        <a href="/konsultasi/edit/<%= getConsultationData[i].id %>" class="btn btn-info btn-sm">
          <i class="fas fa-pencil-alt"></i>
          Ubah
        </a>
        <form action="/kelas/hapus/<%= getConsultationData[i].id %>?_method=DELETE" method="post">
          <a
            href="javascript:void()"
            type="button"
            class="btn btn-danger btn-sm button-delete"
            data-id="<%= getConsultationData[i].id %>"
          >
            <i class="fas fa-trash-alt"></i>
            Hapus
          </a>
        </form>
      </td>
    </tr>
    <% } %>
  </tbody>
</table>
